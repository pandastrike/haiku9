{
  "name": "haiku9",
  "version": "3.0.0",
  "description": "Asset compilation, static-site generator",
  "main": "lib/index.js",
  "files": [
    "bin",
    "configuration-schema",
    "lib",
    "LICENSE"
  ],
  "bin": {
    "h9": "./bin/h9"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "engine-strict": true,
  "dependencies": {
    "ajv": "^6.6.2",
    "aws-sdk": "^2.387.0",
    "commander": "^2.19.0",
    "express": "^4.16.4",
    "js-yaml": "^3.12.1",
    "jstransformer-stylus": "^1.4.0",
    "key-forge": "^0.1.3",
    "panda-parchment": "^4.2.0",
    "panda-quill": "^3.4.5",
    "panda-river": "^4.4.2",
    "sundog": "^2.0.4"
  },
  "devDependencies": {
    "amen": "^1.0.0",
    "json": "^9.0.6",
    "panda-9000": "^4.4.0",
    "panda-builder": "^3.2.1"
  },
  "scripts": {
    "prepare": "coffee -o lib/ -c src/",
    "test": "coffee test/index.coffee",
    "watch": "coffee -o lib/ -cw src/",
    "postpublish": "(json -f package.json version | xargs -I version git tag -am version version) && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pandastrike/haiku9.git"
  },
  "keywords": [
    "blog",
    "jekyll",
    "harp",
    "asset",
    "compilation"
  ],
  "author": "Dan Yoder <dan@pandastrike.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandastrike/haiku9/issues"
  },
  "homepage": "https://github.com/pandastrike/haiku9"
}
